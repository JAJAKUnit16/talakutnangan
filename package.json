{
	"name": "talakutnangan",
	"description": "A capstone project aim to create a consultation platform through chat for MCC",
	"version": "0.1.3-dev",
	"homepage": "https://github.com/lemredd/talakutnangan",
	"keywords": [
		"front-end",
		"web",
		"chat"
	],
	"private": true,
	"scripts": {
		"dev": "nodemon --watch server --ext ts --ignore \"*.spec.ts\" --exec \"npm run server\"",
		"prod": "npm run build && npm run server:prod",
		"build": "vite build && vite build --ssr",
		"server": "ts-node ./server",
		"server:prod": "cross-env NODE_ENV=production ts-node ./server",
		"changelog:generate": "npx changelogen",
		"test:unit:front-end": "npx jest -c jest.front-end.config.json",
		"test:unit:back-end": "npx jest -c jest.back-end.config.json",
		"test:integration:back-end": "npx jest -c jest.back-end.integration.config.json",
		"test:unit:front-end:watch": "npm run test:unit:front-end -- --watch",
		"test:unit:back-end:watch": "npm run test:unit:back-end -- --watch",
		"test:integration:back-end:watch": "npm run test:integration:back-end -- --watch"
	},
	"devDependencies": {
		"@babel/core": "^7",
